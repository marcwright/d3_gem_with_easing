<title>Easing</title>

<script>
  var data = [ // <-A
    "Marc", "cubic", "cubic-in-out", 
    "sin", "sin-out", "exp", "circle", "back", 
    "bounce",
    function(t){ // <-B
      return t * t;
    }
  ],
      colors = d3.scale.category20();

  d3.select("body").selectAll("div")
          .data(data) // <-C
      .enter()
      .append("div")
          .attr("class", "fixed-cell")
          .style("top", function (d, i) {
              return i * 40 + "px";
          })
          .style("background-color", function (d, i) {
              return colors(i);
          })
          .style("color", "white")
          .style("left", "500px")
          .text(function (d) {
              if(typeof d === 'function') return "custom";
              return d;
          });

  d3.selectAll("div").each(function(d){
      d3.select(this)
    .transition().ease(d) // <-D
    .duration(1500)
    .style("left", "10px");
  });
</script>
    




